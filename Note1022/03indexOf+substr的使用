var str="I am from runoob，welcome to runoob site.";
var n=str.lastIndexOf("runoob");
n=28;
n=str.lastIndexOf("r");
n=28;
n=str.indexOf("r");
n=6;
n=str.indexOf("r",11);
n=28;
n=str.lastIndexOf("runoob",28);
n=28;
n=str.lastIndexOf("runoob",29);
n=28;
n=str.lastIndexOf("runoob",27);
n=10;

